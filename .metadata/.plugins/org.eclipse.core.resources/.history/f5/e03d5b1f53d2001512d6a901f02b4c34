package com.cimpapps.part1.chapter1

class FromJtoG implements Runnable {
	
	static main(args){
		
		Thread thread = new Thread(this);
		try {
			thread.start();
		} catch (Exception e) {
			e.printStackTrace()
		}
	}//end of main
		

	public void run() {
		println "Catalin este cel mai tare"
		Thread.sleep(3000)
	}
	
	
}//end of class
